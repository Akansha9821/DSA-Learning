    "def bubblesort(arr):\n",
    "    for j in range(len(arr)-1):\n",
    "        for i in range(len(arr)-1):\n",
    "            if arr[i] > arr[i+1]:\n",
    "                arr[i], arr[i+1] = arr[i+1], arr[i]\n",
    "\n",
    "            \n",
    "    print(arr)\n",
    "    \n",
    "    \n",
    "bubblesort(arr=[10, 15, 4, 23,0])"
   ]
