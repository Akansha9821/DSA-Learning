    "def selection_sort(l):\n",
    "    for i in range(len(l)):\n",
    "        min_ele = min(l[i:])\n",
    "        min_ele_index = l.index(min_ele)\n",
    "        l[i], l[min_ele_index] = l[min_ele_index], l[i]\n",
    "        \n",
    "    return l\n",
    "   \n",
    "    \n",
    "selection_sort(l = [10, 15, 4, 23,0])"
   ]
